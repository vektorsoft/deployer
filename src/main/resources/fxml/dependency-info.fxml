<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vektorsoft.xapps.deployer.ctrl.DependencyController">
   <children>
      <StackPane>
         <children>
            <ScrollPane fx:id="dependenciesPane" fitToWidth="true" prefHeight="345.0" prefWidth="600.0" visible="false">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <TableView fx:id="dependencyTable" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
            <BorderPane fx:id="progressPane">
               <center>
                  <ProgressIndicator fx:id="syncProgressIndicator" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
               </center>
               <bottom>
                  <Label text="Loading dependencies. Please wait..." BorderPane.alignment="CENTER">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </bottom>
            </BorderPane>
         </children>
      </StackPane>
      <HBox fx:id="buttonBar" alignment="CENTER" prefWidth="200.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" text="Remove" />
            <Button mnemonicParsing="false" text="Move to Platform" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
